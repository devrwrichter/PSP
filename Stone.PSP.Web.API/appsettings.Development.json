{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "ShoppingCart"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {    
    "SqlServer": "Server=sqlserver;Database=PaymentServiceProvider;User Id=sa;password=Psp@2023@ps;TrustServerCertificate=True;MultipleActiveResultSets=true;"
  }
}

